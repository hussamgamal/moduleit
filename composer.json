{
    "name": "mshmsh/modules",
    "description": "Seperate your project to modules , every module has all of its functionality to be seperated and reusable",
    "license": "MIT",
    "require": {
        "kreait/laravel-firebase": "*",
        "spatie/laravel-translatable": "*",
        "spatie/laravel-medialibrary": "*",
        "spatie/laravel-permission": "*"
    },
    "authors": [{
        "name": "Hossam MshMsh",
        "email": "hossamgamal.cs@gmail.com"
    }],
    "autoload": {
        "psr-4": {
            "MshMsh\\": "src",
            "Modules\\": "Modules"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MshMsh\\Provider"
            ]
        }
    }
}
